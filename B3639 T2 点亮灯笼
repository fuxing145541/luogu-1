#include <stdio.h>
int main()
{
    int n,m;
    scanf("%d %d",&n,&m);
    int A[n+1],a;
    for(int i=0;i<=n;i++)
    {
        A[i]=0;
    }
    for(int i=0;i<m;i++)
    {
        scanf("%d",&a);
        if(n>=3){
        if(a==1)
        {
            A[a]=!A[a];A[a+1]=!A[a+1];A[n]=!A[n];
        }
        else if(a==n)
        {
            A[a]=!A[a];A[a-1]=!A[a-1];A[1]=!A[1];
        }
        else
        {
            A[a]=!A[a];A[a+1]=!A[a+1];A[a-1]=!A[a-1];
        }
        }
        else 
        {
            for(int j=1;j<=n;j++)
            {
                A[j]=!A[j];
            }
        }
    }
    for(int i=1;i<=n;i++)
    {
        printf("%d ",A[i]);
    }
    return 0;
}

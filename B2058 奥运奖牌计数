#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    
    int x[n][3],i,j;
    for(j=0;j<n;j++){
        for(i=0;i<3;i++)
            scanf("%d",&x[j][i]);
    }
    
    int a=0,b=0,c=0;
    for(i=0;i<n;i++){
    a+=x[i][0];
    b+=x[i][1];
    c+=x[i][2];
    }
        
    int sum;
    sum=a+b+c;
    
    printf("%d %d %d %d",a,b,c,sum);
    return 0;
}

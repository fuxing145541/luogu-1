#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    
    int A[n][2],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<2;j++)
            scanf("%d",&A[i][j]);
    }
    
    float x,y[n-1];
    x=(float)A[0][1]/A[0][0];
    for(i=1;i<n;i++){
        y[i]=(float)A[i][1]/A[i][0];
        if(y[i]-x>0.05) printf("better\n");
        else if(x-y[i]>0.05) printf("worse\n");
        else printf("same\n");
    }
    
    return 0;
}

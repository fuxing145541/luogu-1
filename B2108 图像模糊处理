#include <stdio.h>
#include <stdlib.h>
int aveh(float a)
{
    a=a*10;
    int b,c;
    c=(int)a/10;
    b=(int)a%10;
    if(b>4) c++;
    return c;
}

int main()  
{
    float ave;
    int sum,c;
    int n,m;
    scanf("%d %d",&n,&m);
    int (*Pixel)[m] = malloc(sizeof *Pixel * n);
    int (*Out)[m] = malloc(sizeof *Out * n);
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            scanf("%d",&Pixel[i][j]);
            Out[i][j]=Pixel[i][j];
        }
    }
    for(int i=1;i<n-1;i++)
    {
        for(int j=1;j<m-1;j++)
        {
            sum=Out[i][j]+Out[i][j+1]+Out[i+1][j]+Out[i-1][j]+Out[i][j-1];
            ave=(float)sum/5;
            c=aveh(ave);
            Pixel[i][j]=c;
        }
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            printf("%d ",Pixel[i][j]);
        }
        printf("\n");
    }
    free(Pixel);
    free(Out);
    return 0;
}

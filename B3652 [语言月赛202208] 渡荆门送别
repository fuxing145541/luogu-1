#include <stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    long long e[n];
    for(int i=0;i<n;i++)
    {
        scanf("%lld",&e[i]);
    }
    long long max=e[0];
    long long min=e[0];
    for(int i=0;i<n;i++)
    {
        if(max<e[i])
            max=e[i];
        if(min>e[i])
            min=e[i];
    }

    for(int i=0;i<n;i++)
        printf("%lld ",max-e[i]);
    printf("\n");
    for(int i=0;i<n;i++)
        printf("%lld ",e[i]-min);
    return 0;
}

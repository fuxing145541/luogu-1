#include <stdio.h>
#include <math.h>
int main(){
    double a,b,c,x1,x2;
    scanf("%lf %lf %lf",&a,&b,&c);
    if((pow(b,2)-4*a*c)>0){
        x1=(-b+sqrt(pow(b,2)-4*a*c))/2/a;
        x2=(-b-sqrt(pow(b,2)-4*a*c))/2/a;
        if(x1>x2)
        {
            double t;
            t=x1;
            x1=x2;
            x2=t;
        }
        printf("x1=%.5f;x2=%.5f",x1,x2);
        return 0;
    }
    else if((pow(b,2)-4*a*c)==0)
    {
        x1=(-b+sqrt(pow(b,2)-4*a*c))/2/a;
        x2=(-b-sqrt(pow(b,2)-4*a*c))/2/a;
         printf("x1=x2=%.5f",x2);
    }
    else printf("No answer!");
    return 0;
}

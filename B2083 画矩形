#include<stdio.h>
int main()
{
    int a,b,f;
    char c;
    scanf("%d %d %c %d",&a,&b,&c,&f);
    
    for(int i=1;i<=a;i++)
    {
        for(int j=1;j<=b;j++)
        {
            if(i==1||i==a||f!=0||j==1||j==b)
            { 
                printf("%c",c);
            }
            else
            {
                printf(" ");
                
            }
        }
        printf("\n");
    }
    return 0;

}

#include <stdio.h>
int main()
{
    int a=0,n;
    scanf("%d",&n);
    
    int A[n],i,x=0,g;
    for(i=0;i<n;i++)
    {
        scanf("%d",&A[i]);
        
        x=A[i]%10;
        A[i]=A[i]/10;
        
        for(int j=1;j<=3;j++)
        {
            
            g=A[i]%10;
            A[i]=A[i]/10;
            x-=g;
            
        }
        
        if(x>0) a++;
    }
    printf("%d",a);
    
    return 0;
}

#include <stdio.h>
int main()
{
    int n,x,a;
    scanf("%d",&n);
    int A[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&A[i]);
    }
    scanf("%d",&x);

    for(int i=0;i<n;i++)
    {
        if(x==A[i])
        {
            printf("%d",i);
            return 0;
        }
    }
    printf("-1");
    return 0;
}
